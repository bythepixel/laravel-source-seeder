{
  "name": "bythepixel/nova-page-elements",
  "description": "A base repository template for creating BTP Laravel Packages",
  "homepage": "https://github.com/bythepixel/btp-laravel-package-base",
  "license": "MIT",
  "keywords": [
    "bythepixel",
    "laravel"
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/database": "^7.0|^8.0",
    "illuminate/support": "^7.0|^8.0",
    "laravel/framework": "^7.0|^8.0",
    "laravel/nova": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "PackageName\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "PackageName\\PackageNameServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}